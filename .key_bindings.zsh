# It contains key bindings for the ZSH terminal

# bindkey: <leader>c
# This bindkey is used to open up quick CD access to directories in ZSH
bindkey '\\c' fzf-cd-widget

# bindkey: <leader>h
# This bindkey is used to access history commands in ZSH
bindkey '\\h' fzf-history-widget
